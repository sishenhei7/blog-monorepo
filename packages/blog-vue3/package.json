{
  "name": "blog-vue3",
  "version": "0.0.2-beta.0",
  "description": "vue3 ssr blog, powered by vite",
  "main": "index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon --exec node -r ts-node/register server/index.ts",
    "build:vue": "vue-tsc --noEmit --skipLibCheck && pnpm run build:vue-client && pnpm run build:vue-server",
    "build:vue-client": "vite build --outDir dist/app/client --ssrManifest",
    "build:vue-server": "vite build --outDir dist/app/server --ssr src/entry-server.ts",
    "build:server": "tsup-node server/index.ts -d dist/server",
    "start": "cross-env NODE_ENV=production nodemon --exec node -r ts-node/register server/index.ts",
    "preview": "cross-env NODE_ENV=production node dist/server/index.js",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s --commit-path . --lerna-package layout-vue3",
    "release": "node ../../scripts/release.cjs --skipBuild"
  },
  "author": "yangzhou",
  "license": "ISC",
  "devDependencies": {
    "@css-render/vue3-ssr": "^0.15.6",
    "@types/koa": "^2.13.4",
    "@types/koa-router": "^7.4.4",
    "@types/lru-cache": "^5.1.1",
    "@types/node": "^16.11.11",
    "@vitejs/plugin-vue": "^1.9.3",
    "@vue/compiler-sfc": "^3.2.26",
    "chalk": "^4.1.2",
    "cross-env": "^7.0.3",
    "koa": "^2.13.4",
    "koa-router": "^10.1.1",
    "lru-cache": "^6.0.0",
    "nodemon": "^2.0.15",
    "ora": "^6.0.1",
    "redis": "^4.0.1",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.12.0",
    "tsup": "^5.11.9",
    "typescript": "^4.5.4",
    "vite": "^2.6.4",
    "vue-tsc": "^0.3.0"
  },
  "dependencies": {
    "naive-ui": "^2.23.1",
    "vue": "^3.2.26",
    "vue-i18n": "^9.2.0-beta.25",
    "vue-router": "^4.0.0"
  }
}
